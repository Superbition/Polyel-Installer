{
    "name": "superbition/polyel-installer",
    "description": "The Polyel PHP framework installer",
    "keywords": ["polyel", "framework", "async", "coroutine", "swoole"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Luke Embrey",
            "email": "hello@polyel.io"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/polyel"
    ],
    "autoload": {
        "psr-4": {
            "Polyel\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polyel\\Installer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
